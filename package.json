    "path-match": "^1.2.4",
    "qs": "^6.3.0",
    "ramda": "^0.25.0",
    "raw-body": "^2.2.0",
    "send": "^0.16.0"
  },
  "devDependencies": {
    "@articulate/spy": "^0.0.1",
    "chai": "^3.5.0",
    "coveralls": "^3.0.3",
    "crocks": "^0.10.1",
    "eslint": "^5.15.1",
    "http-errors": "^1.6.1",
    "joi": "^14.3.1",
    "levelup": "^1.3.3",
    "memdown": "^1.2.4",
