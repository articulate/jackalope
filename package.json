  "author": "articulate",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100
  },
  "scripts": {
